{
  "name": "websocket-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run server\" \"pnpm run client\"",
    "server": "cd server && pnpm run start:dev",
    "client": "cd client && pnpm run start:dev",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "cd client && pnpm run build",
    "build:server": "cd server && pnpm run build",
    "start": "cd server && pnpm run start:prod",
    "install:all": "pnpm install && cd client && pnpm install && cd ../server && pnpm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
